{
  "manifest_version": 2,

  "name": "Deployer",
  "description": "This extension allows us to deploy branches to staging with the click of a button.",
  "version": "1.0",

  "permissions": [
    "https://github.com/"
  ],
  "content_scripts": [
    {
      "matches": ["https://github.com/Thrillist/*/pull/*"],
      "js": ["myscript.js"],
      "css": ["mycss.css"]
    }
  ],
  "web_accessible_resources": ["mycss.css"]
}